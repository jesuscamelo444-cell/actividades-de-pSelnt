Algoritmo ejecicioXIII
	// Definir tipo de varible
	Definir ingreso Como Real;
	Definir ganacia Como Real;
	Definir gastos Como Real
	
	// 2 tomar datos o asignar varibles
	Escribir "ingreso bruto de 2.000.000";
	ingreso <- 2000000 
	Escribir " gastos 500.000"
	gastos <- 500000
	
	// 3 procesar datos
	ganacia <- ingreso - gastos
	
	// 4  mostrar resultados
	Escribir " la ganacia neta es de: $", ganacia
FinAlgoritmo
